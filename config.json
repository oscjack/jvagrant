{
    "ip": "192.168.2.10",
    "memory": 2048,
    "cpus": 2,
    "name": "jvagrant",
    "provider": "virtualbox",
    "authorize": "~/.ssh/id_rsa.pub",
    "keys": [
        "~/.ssh/id_rsa"
    ],
    "folders": [
        {
            "map": "./test",
            "to": "/var/test"
        }
    ],
    "sites": [
        {
            "map": "test.me",
            "to": "/var/test"
        }
    ],
    "ports": [
        {
            "send": 3360,
            "to": 3306
        }
    ],
    "provisions": [
        "install-lamp"
    ]
}
